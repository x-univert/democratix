    "circomlibjs": "^0.1.7",
    "classnames": "2.3.2",
    "html2canvas": "^1.4.1",
    "i18next": "^25.6.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "moment": "2.29.4",
    "papaparse": "^5.5.3",
