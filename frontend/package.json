{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:devnet",
    "dev:devnet": "npm run copy:devnet-config && vite",
    "dev:testnet": "npm run copy:testnet-config && vite",
    "dev:mainnet": "npm run copy:mainnet-config && vite",
    "build": "npm run build:devnet",
    "build:devnet": "npm run copy:devnet-config && vite build",
    "build:testnet": "npm run copy:testnet-config && vite build",
    "build:mainnet": "npm run copy:mainnet-config && vite build",
    "type-check": "tsc -b",
    "copy:devnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.devnet.ts', './src/config/index.ts')\"",
    "copy:testnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.testnet.ts', './src/config/index.ts')\"",
    "copy:mainnet-config": "node -e \"require('fs').copyFileSync('./src/config/config.mainnet.ts', './src/config/index.ts')\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headed": "cypress run --headed"
  },
